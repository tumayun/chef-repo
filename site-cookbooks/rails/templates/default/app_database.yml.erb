<%= @rails_env %>:
  adapter: <%= @database_info['adapter'] %>
  host: <%= @database_info['host'] %>
  username: <%= @database_info['username'] %>
  password: <%= @database_info['password'] %>
  pool: 20
  timeout: 5000
  database: <%= @database_info['database'] %>
  charset: utf8mb4
  encoding: utf8mb4
  collation: utf8mb4_unicode_ci
